<h1><strong>Bookings</strong></h1>
<ul class="list-group">
  <% @bookings.each do |booking| %>
    <li class="list-group-item"><%= booking.start_date%></li>
    <li class="list-group-item"><%= booking.end_date%></li>
    <li><%= booking.fane.title %></li>
    <%= link_to "delete" , booking_path(booking), data: {turbo_method: :delete, turbo_confirm: "Are you sure?"},  class:"btn btn-primary"%>
  <% end %>
</ul>
<br>
